{
  "name": "jss-nuxt-test",
  "version": "1.0.0",
  "description": "Test nuxt app for integrating JSS",
  "author": "Sam Garson",
  "private": true,
  "config": {
    "appName": "jss-nuxt-test",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreDistPath": "/dist/jss-test",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
